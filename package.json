{
	"name": "ccc-trainer-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"supabase-gen-types": "supabase gen types typescript --project-id noocroigqdmmgfvphkfl > src/lib/supabase/database.types.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@skeletonlabs/skeleton": "^2.4.0",
		"@skeletonlabs/tw-plugin": "^0.2.3",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.3",
		"@types/node": "^22.1.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.2.2",
		"vite": "^4.4.2",
		"vite-plugin-tailwind-purgecss": "^0.1.3",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "^2.38.4",
		"supabase": "^1.110.1",
		"svelte-particles": "^2.12.0",
		"svelte-preprocess": "^5.0.4",
		"tsparticles": "^2.12.0"
	},
	"overrides": {
		"svelte-chartjs": {
			"svelte": ">=4.x"
		}
	}
}
